    - import stemmer.PorterStemmer
    %thead>
      %tr>
        %th
          Input case
        %th
          First word stemmed
        %th
          Second word stemmed

    %tbody
      %tr
        %td
          <strong>European</strong> should not stem to the same as <strong>Europe</strong>
        %td
          = PorterStemmer("European")("English")
        %td
          = PorterStemmer("Europe")("English")
      %tr
        %td
          <strong>Cylinder</strong> should not stem to the same as <strong>Cylindrical</strong>
        %td
          = PorterStemmer("Cylinder")("English")
        %td
          = PorterStemmer("Cylindrical")("English")
      %tr
        %td
          <strong>Matrices</strong> should not stem to the same as <strong>Matrix</strong>
        %td
          = PorterStemmer("Matrices")("English")
        %td
          = PorterStemmer("Matrix")("English")
      %tr
        %td
          <strong>Urgency</strong> should not stem to the same as <strong>Urgent</strong>
        %td
          = PorterStemmer("Urgency")("English")
        %td
          = PorterStemmer("Urgent")("English")
      %tr
        %td
          <strong>Create</strong> should not stem to the same as <strong>Creation</strong>
        %td
          = PorterStemmer("Create")("English")
        %td
          = PorterStemmer("Creation")("English")

      %tr
        %td
          <strong>Analysis</strong> should not stem to the same as <strong>Analyses</strong>
        %td
          = PorterStemmer("Analysis")("English")
        %td
          = PorterStemmer("Analyses")("English")

